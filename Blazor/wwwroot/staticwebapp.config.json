{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*",
            "/css/*",
            "/js/*",
            "/favicon.ico",
            "/*.woff2",
            "/*.woff",
            "/*.ttf",
            "/*.png",
            "/*.jpg",
            "/*.svg",
            "/*.webmanifest"
        ]
    },
    "mimeTypes": {
        ".html": "text/html; charset=utf-8",
        ".css": "text/css; charset=utf-8",
        ".js": "application/javascript; charset=utf-8"
    }
}